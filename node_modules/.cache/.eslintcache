   const wrapperRefProfile = useRef(null);\n   useOutsideAlerter(wrapperRefProfile, \"Profile\");\n\n   function useOutsideAlerter(ref: any, component: String) {\n       useEffect(() => {\n           function handleClickOutside(event: any) {\n               if(component === \"Profile\") {\n                   if(ref.current && !ref.current.contains(event.target)){\n                       setTimeout(() => setDropdownProfileClass(\"hidden\"), 100);\n                   }\n               }\n           }\n\n           // Bind the event listener\n           document.addEventListener(\"mousedown\", handleClickOutside);\n           return () => {\n               // Unbind the event listener on clean up\n               document.removeEventListener(\"mousedown\", handleClickOutside);\n           };\n   }, [ref, component]);}\n\n   const MenuItem = (props: any) => {\n    const isActive = window.location.pathname === props.link;\n\n    return (\n      <Navbtn text={props.text} />\n    )\n   }\n\n   const MenuIcon = () => {\n    if(!props.sidebarEnabled) {\n      return (\n        <HiMenu className=\"text-2xl\" />\n      )\n    } else {\n      return (\n        <HiX className=\"text-2xl\" />\n      )\n    }\n   }\n\n  return (\n    <div className=\"relative w-max bg-white border-b \">\n      <img className=\"absolute left-0 ml-5 h-16\" src=\"https://cdn.logo.com/hotlink-ok/logo-social.png\" alt=\"\"/>\n      <div className=\"flex h-16 flex-row justify-end\">\n        {(\n          <>\n            {MenuData.map((item, val) => (\n                <Link to={item.link} key={val} onClick={()=>{setItemActive(item.title)}}>\n                    <MenuItem key={val} link={item.link} text={item.title} />\n                </Link>\n            ))}\n            <button ref={wrapperRefProfile} className=\"px-3 py-3 mx-2 my-2 rounded-lg hover:bg-gray-200 focus:outline-none\" \n                onClick={() => {setDropdownProfileClass(\"block\")}}>\n              <HiOutlineUser className=\"text-2xl\"/>\n            </button>\n            <ProfileDropdown userLogged={props.userLogged} class={dropdownProfileclass}/>\n            <button className=\"px-3 py-3 mx-2 my-2 rounded-lg hover:bg-gray-200 focus:outline-none sm:hidden\"\n                onClick={() => {props.setSidebarEnabled(!props.sidebarEnabled)}}>\n              {MenuIcon()}\n            </button>\n          </>\n        )}\n      </div>\n\n    </div>\n  )\n}\n\nexport default AppBar;",[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/components/MenuData.js",[],[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/pages/home/Home.tsx",[],[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/pages/reviews/Reviews.tsx",[],[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/pages/ranking/Ranking.tsx",[],[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/pages/profile/Profile.tsx",[],[],"/Users/domenicolorenti/NTTStage/itinera/fe-itinera/src/pages/explore/Explore.tsx",[],[],{"ruleId":"69","severity":1,"message":"70","line":10,"column":22,"nodeType":"71","messageId":"72","endLine":10,"endColumn":35},{"ruleId":"69","severity":1,"message":"73","line":14,"column":11,"nodeType":"71","messageId":"72","endLine":14,"endColumn":19},{"ruleId":"69","severity":1,"message":"74","line":28,"column":12,"nodeType":"71","messageId":"72","endLine":28,"endColumn":22},{"ruleId":"69","severity":1,"message":"74","line":39,"column":11,"nodeType":"71","messageId":"72","endLine":39,"endColumn":21},{"ruleId":"69","severity":1,"message":"73","line":63,"column":11,"nodeType":"71","messageId":"72","endLine":63,"endColumn":19},"@typescript-eslint/no-unused-vars","'setUserLogged' is assigned a value but never used.","Identifier","unusedVar","'isActive' is assigned a value but never used.","'itemActive' is assigned a value but never used."]